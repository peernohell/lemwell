{
  "manifest_version": 2,
  "name": "lemwell",
  "description": "Chrome extension that provide a Profitwell dashboard on new page",
  "version": "1.0.1",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
   "128": "icon128.png"
  },
  "permissions": [
    "https://api.profitwell.com/v2/metrics/monthly/",
    "https://api.profitwell.com/v2/metrics/daily/",
    "https://api.profitwell.com/v2/metrics/plans/"
  ]
}